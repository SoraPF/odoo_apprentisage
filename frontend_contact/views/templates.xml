<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="list_contact_page" name="lcp">
            <body>
                <h1>Contact</h1>
                <h1 class="text-3xl font-bold underline">
                    Hello world!
                  </h1>
                <input type="text" placeholder="Search teams or members" id="search_bar"/>

                <table border="1" id="table">
                    <tr>
                        <td>Nom</td>
                        <td>Mobile</td>
                    </tr>
                    <t t-foreach="contact" t-as="ct">
                        <tr>
                            <td>
                                <t t-esc="ct.name"/>
                            </td>
                            <td>
                                <t t-esc="ct.mobile"/>
                            </td>
                        </tr>
                    </t>
                </table>
            </body>

                <script type="text/javascript" src="/frontend_contact/static/javascript.js"/>
                <link rel="stylesheet" href="/frontend_contact/static/output.css"/>
        </template>
    </data>
</odoo>